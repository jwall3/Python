##############################################################################
def fs(l,i, j):
    global h

    f = open ("rama_ic.xvg", "r")

    for line in f:
        data = f.readlines()
    
    h = []
    v = 0
    while v < l:
        
        dx = data[v].split()
        h.append(dx[i])
        #print(dx[0])
        v+=1
    print(h)
    print(h[0])
    
##############################################################################

fs(10,2, "f1")


print(h)













